<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AppChamaGas.View.PessoaView">
    <ContentPage.Content>
        <StackLayout>
            <TableView>
                <TableRoot Title="Pessoa">
                    <TableSection>
                        <ViewCell>
                            <Entry x:Name="etCEP" Placeholder="Informe o CEP" Unfocused="EtCEP_Unfocused"></Entry>
                        </ViewCell>
                        <EntryCell x:Name="etLogradouro" Placeholder="Logradouro" />
                        <EntryCell x:Name="etComplemento" Placeholder="Complemento"/>
                        <EntryCell x:Name="etBairro" Placeholder="Bairro" />
                        <EntryCell x:Name="etLocalidade" Placeholder="Localidade" />
                        <EntryCell x:Name="etUF" Placeholder="UF" />
                    </TableSection>
                </TableRoot>
            </TableView>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>